{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "types": [
      "node"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "./src/**/*",
    "./src/@types/**/*",
    "index.d.ts",
    ".eslintrc.js",
    "babel.config.js",
    "index.js",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "./src/components/reusables/primitives/**/*"
  ]
}